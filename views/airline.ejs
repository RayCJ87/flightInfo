<html>
    <head>
        <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

        <script src="http://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

        <!-- Bootstrap CSS -->
        <!-- <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous"> -->


        <!-- jquery / app.js -->
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
        <script type="text/javascript" src="/scripts/app.js"></script>
        <!-- CSS stylesheets -->
        <!-- <link rel="stylesheet" href="/styles/index.css" type="text/css" /> -->

    </head>

    <body>


        <div>
            <table id="flightInfo">
                <tr>
                    <th>Flight Number</th>
                    <th>Departure</th>
                    <th>Arrival</th>
                    <th>Latitude</th>
                    <th>Longitude</th>
                    <th>Altitude</th>
                    <th>Speed</th>
                    
                </tr>
                <!-- <tr> -->
                   <% flightData.forEach(function(item) { %>
                    <tr class="infoContainer">
                        <!-- <form method="POST" action="/result"> -->
                                <td><%= item.flight.iataNumber %></td>
                                <td><%= item.departure.iataCode %></td>
                                <td id="iaCode"><%= item.arrival.iataCode %></td>
                                <td class='tLat'><%= item.geography.latitude %></td>
                                <td id='tLon'><%= item.geography.longitude %></td>
                                <td><%= item.geography.altitude %></td>
                                <td><%= item.speed.horizontal %></td>
                                <td>

                                    <input id="theNumber" type="hidden" value="<%= item.flight.iataNumber %>" name="flightNum"> 
                                    <input id="theLat" type="hidden"  value= "<%= item.geography.latitude %>" name="flightLat" >
                                    <input id="theLong" type="hidden"  value= "<%= item.geography.longitude %>" name="flightLong" >
                                    <input id="theAlti" type="hidden"  value= "<%= item.geography.altitude %>" name="flightAlti" >
                                    <input id="theSpeed" type="hidden"  value= "<%= item.speed.horizontal %>" name="flightSpeed" >
                                    <input id="theArr" type="hidden"  value= "<%= item.arrival.iataCode %>" name="flightArr" >
                                    <button type="button" class="btn btn-primary" id="showButton">Show</button>
                                </td>
                        <!-- </form> -->
                    </tr>
                    <% });%>
                <!-- </tr> -->
            </table>
        </div>




    </body>

</html>